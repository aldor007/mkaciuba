{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "name": "category"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "slug": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "slugOverride": {
      "type": "string",
      "required": false
    },
    "public": {
      "type": "boolean"
    },
    "publicationDate": {
      "type": "datetime",
      "required": true
    },
    "file": {
      "type": "media",
      "allowedTypes": [
        "files"
      ],
      "multiple": false,
      "required": false
    },
    "image": {
      "type": "media",
      "allowedTypes": [
        "images"
      ],
      "multiple": false,
      "required": true
    },
    "medias": {
      "type": "media",
      "allowedTypes": [
        "images"
      ],
      "multiple": true,
      "required": true
    },
    "gallery": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::gallery.gallery",
      "inversedBy": "categories"
    },
    "keywords": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "text": {
      "type": "richtext"
    },
    "token": {
      "type": "password",
      "private": true
    }
  }
}
