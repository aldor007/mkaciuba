{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "name": "Category",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "slug": {
      "type": "string",
      "required": false,
      "unique": false
    },
    "public": {
      "type": "boolean"
    },
    "publicationDate": {
      "type": "datetime",
      "required": true
    },
    "file": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": false
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "medias": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "categories",
      "dominant": true
    },
    "gallery": {
      "model": "gallery",
      "via": "categories"
    }
  }
}
