{
  "version": 1,
  "projects": {
    "photos": {
      "root": "apps/photos",
      "sourceRoot": "apps/photos/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/photos",
            "index": "apps/photos/src/index.html",
            "main": "apps/photos/src/main.tsx",
            "polyfills": "apps/photos/src/polyfills.ts",
            "tsConfig": "apps/photos/tsconfig.app.json",
            "assets": [
              "apps/photos/src/favicon.ico",
              "apps/photos/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "apps/photos/webpack.config.ts",
            "buildLibsFromSource": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/photos/src/environments/environment.ts",
                  "with": "apps/photos/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "photos:build",
            "proxyConfig": "apps/photos/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "photos:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/photos/**/*.js",
              "apps/photos/**/*.jsx",
              "apps/photos/**/*.ts",
              "apps/photos/**/*.tsx",
              "apps/photos/**/*.spec.ts",
              "apps/photos/**/*.spec.tsx",
              "apps/photos/**/*.spec.js",
              "apps/photos/**/*.spec.jsx",
              "apps/photos/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/photos/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/photos"
          ]
        }
      }
    },
    "photos-e2e": {
      "root": "apps/photos-e2e",
      "sourceRoot": "apps/photos-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/photos-e2e/cypress.json",
            "tsConfig": "apps/photos-e2e/tsconfig.e2e.json",
            "devServerTarget": "photos:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "photos:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
