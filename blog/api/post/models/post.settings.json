{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "text": {
      "type": "richtext"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "publicationDate": {
      "type": "datetime",
      "required": true
    },
    "gallery": {
      "model": "category"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "keywords": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "category": {
      "model": "post-category"
    },
    "slug": {
      "type": "string",
      "unique": true,
      "private": false
    },
    "permalink": {
      "type": "string"
    },
    "content_position": {
      "type": "enumeration",
      "enum": [
        "top",
        "bottom"
      ]
    },
    "gallery_template": {
      "type": "enumeration",
      "enum": [
        "normal",
        "description"
      ]
    },
    "tags": {
      "collection": "tag",
      "via": "posts",
      "dominant": true
    }
  }
}
